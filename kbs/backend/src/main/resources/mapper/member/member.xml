<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kimbap.kbs.mapper.MemberMapper">

<select id="getUserInfo" resultType="MemberVO"> <!-- 로그인 -->
		SELECT   a.id
                    ,a.pw
                    ,b.emp_name
                    ,b.tel
		FROM    a t_member JOIN b t_employee
    ON      t_member.emp_code = t_employee.emp_code
    WHERE   id = #{id}
	</select>
</mapper>